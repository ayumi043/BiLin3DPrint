{
  "dependencies": {
    "log4net": "2.0.6",
    "Microsoft.AspNet.Razor": "3.2.3",
    "Microsoft.AspNetCore.Diagnostics": "1.1.0",
    "Microsoft.AspNetCore.Owin": "1.1.0",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.1.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.1.0",
    "Microsoft.Extensions.Configuration.Json": "1.1.0",
    "Microsoft.Extensions.Logging.Console": "1.1.0",
    "MySql.Data": "6.9.9",
    "Nancy": "2.0.0-clinteastwood",
    "Nancy.Authentication.Forms": "2.0.0-clinteastwood",
    "Nancy.Validation.DataAnnotations": "2.0.0-clinteastwood",
    "Nancy.Viewengines.Razor":  "2.0.0-clinteastwood",
    "ServiceStack.OrmLite.MySql": "4.5.4",
    "System.ComponentModel.Annotations": "4.1.0",
    "System.Data.Common": "4.3.0",
    "ThoughtWorks.QRCode": "1.1.0",
    "Yiauo.Three": "1.0.1"
  },

  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.1.0-preview4-final"
  },

  "frameworks": {
    "net462": {
      "frameworkAssemblies": {
        "System.Drawing": "4.0.0.0"
      }
    }
  },

  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true
  },

  "publishOptions": {
    "include": [
      "wwwroot",
      "**/*.cshtml",
      "appsettings.json",
      "web.config",
      "Public",
      "app.config",
      "log4net.config"
    ]
  },

  "scripts": {
    "postpublish": [ "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%" ]
  }
}
