@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<dynamic>
@using Nancy.Validation;
@using Bilin3d.HtmlExtensions;
@using System.Collections.Generic;

@{
    Layout = "_Layout.cshtml";
}

@section BreadCrumbs {
    <div class="breadcrumbs">
        <div class="container">
            <h1 class="pull-left">付款成功</h1>
            <ul class="pull-right breadcrumb">
                <li><a href="/">首页</a></li>
                <li class="active">付款成功</li>
            </ul>
        </div><!--/container-->
    </div>
}

@section Acount {
    @Html.Partial("_Acount.cshtml")
}

<div class="row" style="padding:0px 15px 0px 15px;margin-top:-30px;">
    <div>
        <div class="modal-content" style="box-shadow:none;">
            <div class="modal-header">
                <h4 id="myModalLabel1" class="modal-title">完成订单支付</h4>
            </div>
            <div class="modal-body">
                <p style="font-size:14px;">您已完成订单支付，等待管理员审核确认！ &nbsp;&nbsp;点击返回<a href="./order">订单首页</a>可查看订单状态。</p>
            </div>
        </div>
    </div>
  

</div>